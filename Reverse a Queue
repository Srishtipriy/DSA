# Reverse a Queue 

#include <bits/stdc++.h> 
using namespace std;

void reverseHelper(queue<int> &q) {
    if(q.empty()) return;

    int element = q.front();
    q.pop();

    reverseHelper(q);

    q.push(element);
}

queue<int> reverseQueue(queue<int> q) {
    reverseHelper(q);
    return q;
}
